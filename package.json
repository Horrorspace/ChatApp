{
  "name": "chat-app",
  "version": "1.0.0",
  "description": "Fullstack application to messaging",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "ts": "tsc --watch --resolveJsonModule",
    "serve": "cross-env NODE_ENV=development nodemon ./build/src/server/main.js",
    "production": "cross-env NODE_ENV=production node ./build/src/server/main.js",
    "ts-build": "tsc --resolveJsonModule",
    "dev-start": "cross-env NODE_ENV=development webpack serve --mode development --open",
    "db-create": "cd sql psql -U postgres -W -f delete.sql",
    "db-delete": "cd sql psql -U postgres -W -f delete.sql",
    "prebuild": "rimraf dist",
    "nest-build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Horrorspace/ChatApp.git"
  },
  "keywords": [
    "ts",
    "typescript",
    "react"
  ],
  "author": {
    "name": "eugenii",
    "email": "utkineugeniy@gmail.com"
  },
  "private": true,
  "license": "SEE LICENSE IN <LICENSE>",
  "bugs": {
    "url": "https://github.com/Horrorspace/ChatApp/issues"
  },
  "homepage": "https://github.com/Horrorspace/ChatApp#readme",
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.34",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "antd": "^4.16.13",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
